{
  "name": "commish-client",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://mttwlkr.github.io/commish-client",
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@types/jest": "^25.2.1",
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "formik": "^2.1.4",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.2.0",
    "typescript": "^3.9.2",
    "web3": "^1.2.7",
    "yup": "^0.29.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "types-truffle": "npx typechain --target truffle-v5 'src/contracts/**/*.json'",
    "types-web3": "npx typechain --target web3-v1 'src/contracts/**/*.json'",
    "types": "npm run types-truffle && npm run types-web3",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typechain/truffle-v5": "^2.0.2",
    "@typechain/web3-v1": "^1.0.0",
    "@types/yup": "^0.29.3",
    "gh-pages": "^3.1.0",
    "typechain": "^2.0.0"
  }
}
